        "apollo-link-http": "^1.5.17",
        "graphql": "^15.0.0",
        "graphql-tag": "^2.10.3",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "styled-components": "^5.1.1"
    },
    "devDependencies": {

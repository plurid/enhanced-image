{
    "name": "@plurid/enhanced-image-chrome",
    "version": "0.1.0",
    "description": "Chrome Extension for Enhanced Image HTML Element",
    "keywords": [
        "enhanced",
        "image",
        "html"
    ],
    "author": "Jan Cavel <caveljan@plurid.com> (https://plurid.com)",
    "license": "MIT",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/enhanced-image"
    },
    "bugs": {
        "email": "feedback@plurid.com",
        "url": "https://github.com/plurid/enhanced-image/issues"
    },
    "homepage": "https://plurid.com/depict",
    "files": [
        "dist/"
    ],
    "scripts": {
        "clean": "rm -rf dist && rm -rf build && rm -rf dist-zip",
        "dev": "yarn clean && NODE_ENV=development webpack --watch",
        "build": "yarn clean && NODE_ENV=production webpack",
        "distribute": "yarn clean && NODE_ENV=production webpack && node scripts/zip.js",
        "test": "npm run unit-test && npm run e2e-test",
        "test.production": "NODE_ENV=production npm run unit-test && npm run e2e-test",
        "unit-test": "jest __tests__/.*.spec.js --silent",
        "e2e-test": "jest __e2e-tests__ --runInBand --silent",
        "test.jest": "jest",
        "lint": "eslint --quiet -f codeframe src"
    },
    "dependencies": {
        "@plurid/enhanced-image-react": "^0.1.28",
        "@plurid/plurid-functions": "^0.1.1",
        "@plurid/plurid-themes": "^0.1.0",
        "apollo-cache-inmemory": "^1.6.3",
        "apollo-client": "^2.6.4",
        "apollo-link-http": "^1.5.16",
        "graphql": "^14.5.8",
        "graphql-tag": "^2.10.1",
        "react": "^16.11.0",
        "react-dom": "^16.11.0",
        "styled-components": "^4.4.1"
    },
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/preset-env": "^7.6.3",
        "@babel/preset-react": "^7.6.3",
        "@babel/preset-typescript": "^7.6.0",
        "@babel/register": "^7.6.2",
        "@types/chrome": "^0.0.91",
        "@types/jest": "^24.0.21",
        "@types/node": "^12.12.5",
        "@types/react": "^16.9.11",
        "@types/react-dom": "^16.9.3",
        "@types/styled-components": "^4.1.20",
        "copy-webpack-plugin": "^5.0.4",
        "css-loader": "^3.2.0",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.9.0",
        "ts-jest": "^24.1.0",
        "ts-loader": "^6.2.1",
        "typescript": "^3.6.4",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10",
        "zip-folder": "^1.0.0"
    }
}

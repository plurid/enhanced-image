        "@types/react-dom": "^16.9.8",
        "@types/styled-components": "^5.1.0",
        "copy-webpack-plugin": "^6.0.2",
        "css-loader": "^3.5.3",
        "html-webpack-plugin": "^4.3.0",
        "jest": "^26.0.1",

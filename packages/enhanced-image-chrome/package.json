{
    "name": "@plurid/enhanced-image-chrome",
    "version": "0.1.0",
    "description": "Chrome Extension for Enhanced Image HTML Element",
    "keywords": [
        "enhanced",
        "image",
        "html"
    ],
    "author": "Jan Cavel <caveljan@plurid.com> (https://plurid.com)",
    "license": "MIT",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/enhanced-image"
    },
    "bugs": {
        "email": "feedback@plurid.com",
        "url": "https://github.com/plurid/enhanced-image/issues"
    },
    "homepage": "https://plurid.com/depict",
    "files": [
        "distribution/"
    ],
    "scripts": {
        "e2e-test": "jest __e2e-tests__ --runInBand --silent",
        "unit-test": "jest __tests__/.*.spec.js --silent",
        "test": "npm run unit-test && npm run e2e-test",
        "test.jest": "jest",
        "test.production": "NODE_ENV=production npm run unit-test && npm run e2e-test",
        "lint": "eslint --quiet -f codeframe src",
        "clean": "rm -rf distribution && rm -rf distribution-zip && rm -rf build",
        "develop": "yarn clean && NODE_ENV=development webpack --watch",
        "build": "yarn clean && NODE_ENV=production webpack",
        "distribute": "yarn clean && NODE_ENV=production webpack && node scripts/zip.js"
    },
    "dependencies": {
        "@plurid/enhanced-image-react": "^0.1.36",
        "@plurid/plurid-functions": "^0.1.7",
        "@plurid/plurid-themes": "^0.1.26",
        "@plurid/plurid-ui-react": "^0.1.75",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-client": "^2.6.8",
        "apollo-link-http": "^1.5.16",
        "graphql": "^14.6.0",
        "graphql-tag": "^2.10.2",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "styled-components": "^5.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-react": "^7.8.3",
        "@babel/preset-typescript": "^7.8.3",
        "@babel/register": "^7.8.3",
        "@types/chrome": "^0.0.93",
        "@types/jest": "^25.1.1",
        "@types/node": "^13.7.0",
        "@types/react": "^16.9.19",
        "@types/react-dom": "^16.9.5",
        "@types/styled-components": "^4.4.2",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.4.2",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^25.1.0",
        "ts-jest": "^25.2.0",
        "ts-loader": "^6.2.1",
        "typescript": "^3.7.5",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "zip-folder": "^1.0.0"
    }
}
